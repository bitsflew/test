<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Title</key>
	<string>Anti-static questions</string>
	<key>Questions</key>
	<array>
		<dict>
			<key>Key</key>
			<string>blooming_concern</string>
			<key>Title</key>
			<string>Is blooming for post-conversion processes a concern?</string>
			<key>Class</key>
			<string>Switch</string>
		</dict>
		<dict>
			<key>Key</key>
			<string>humidity</string>
			<key>Title</key>
			<string>What humidity will the application operate in? [15% ~ 60%]</string>
			<key>Class</key>
			<string>TextField</string>
			<key>Attributes</key>
			<dict>
				<key>Numeric</key>
				<true/>
			</dict>
		</dict>
		<dict>
			<key>Key</key>
			<string>environment</string>
			<key>Title</key>
			<string>What specific environment is being targeted?</string>
			<key>Class</key>
			<string>Checklist</string>
			<key>Attributes</key>
			<dict>
				<key>MultiSelect</key>
				<false/>
				<key>Items</key>
				<array>
					<dict>
						<key>Title</key>
						<string>Explosion-prone</string>
						<key>Key</key>
						<string>explosion_prone</string>
					</dict>
					<dict>
						<key>Title</key>
						<string>Computer rooms</string>
						<key>Key</key>
						<string>computer_rooms</string>
					</dict>
					<dict>
						<key>Title</key>
						<string>Electrical codes</string>
						<key>Key</key>
						<string>electrical_codes</string>
					</dict>
					<dict>
						<key>Title</key>
						<string>Dust control</string>
						<key>Key</key>
						<string>dust_control</string>
					</dict>
				</array>
			</dict>
		</dict>
		<dict>
			<key>Key</key>
			<string>surfaced_resistivity</string>
			<key>Title</key>
			<string>What surfaced resistivity is required?</string>
			<key>Class</key>
			<string>TextField</string>
			<key>Attributes</key>
			<dict>
				<key>Numeric</key>
				<true/>
			</dict>
		</dict>
		<dict>
			<key>Key</key>
			<string>static_decay_time</string>
			<key>Title</key>
			<string>Static decay requirement</string>
			<key>Class</key>
			<string>TextField</string>
			<key>Attributes</key>
			<dict>
				<key>Prompt</key>
				<string>seconds</string>
				<key>Numeric</key>
				<true/>
			</dict>
		</dict>
		<dict>
			<key>Key</key>
			<string>static_decay_humidity</string>
			<key>Title</key>
			<string>...at relative humidity</string>
			<key>Class</key>
			<string>TextField</string>
			<key>Attributes</key>
			<dict>
				<key>Prompt</key>
				<string>percentage</string>
				<key>Numeric</key>
				<true/>
			</dict>
		</dict>
	</array>
</dict>
</plist>
